<style>
  .page-contact .map-wrapper {
    justify-content: center;
    display: flex;
    padding-bottom: 20px;
  }

  .list-contact {
    margin-top: 20px;
    padding: 20px;
  }

  .list-contact p {
    margin-bottom: 10px;
  }

  .list-contact p i {
    margin-right: 10px;
  }

  .list-contact p i.fa {
    color: #003c96;
  }

  .list-contact p,
  h2 {
    text-align: center;
  }
</style>
<div class="container bg-secondary-subtle rounded-4 page-contact mb-3">
  <div class="contact-text">
    <div class="">
      <div class="list-contact">
        <h2>CÔNG TY CỔ PHẦN THIẾT BỊ ĐIỆN AN PHÁT SÀI GÒN</h2>

        <p>
          <i class="fa fa-map-marker mr5" aria-hidden="true"></i><%=
          setting?.contact.address %>
        </p>

        <p>
          <i class="fa fa-phone mr5" aria-hidden="true"></i> <%=
          setting?.contact.phone %>
        </p>

        <p>
          <i class="fa fa-envelope" aria-hidden="true"></i> <%=
          setting?.contact.email %>
        </p>
      </div>
    </div>
    <div class="map-wrapper">
      <div
        style="
          max-width: 100%;
          overflow: hidden;
          color: red;
          width: 100%;
          height: 400px;
        ">
        <div
          id="google-maps-display"
          style="height: 100%; width: 100%; max-width: 100%">
          <iframe
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3919.813768244475!2d106.6160407!3d10.7488318!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x31752dcd6a794a93%3A0x3fb01abad4f37c07!2zMTg3IMSQLiBz4buRIDMsIELDrG5oIFRy4buLIMSQw7RuZyBCLCBCw6xuaCBUw6JuLCBUaMOgbmggcGjhu5EgSOG7kyBDaMOtIE1pbmg!5e0!3m2!1svi!2s!4v1690537389144!5m2!1svi!2s"
            style="height: 100%; width: 100%; border-radius: 10px"
            allowfullscreen=""
            loading="lazy"
            referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
        <a
          class="our-googlemap-code"
          href="https://www.bootstrapskins.com/themes"
          id="authmaps-data"
          >premium bootstrap themes</a
        ><style>
          #google-maps-display img {
            max-height: none;
            max-width: none !important;
            background: none !important;
          }
        </style>
      </div>
    </div>
  </div>
</div>
